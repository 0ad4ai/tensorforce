{
  "loglevel": "debug",
  "memory_capacity": 10000,
  "batch_size": 32,
  "update_rate": 0.25,
  "update_repeat": 1,
  "use_target_network": true,
  "target_update_frequency": 10000,
  "first_update": 5e4,
  "learning_rate": 0.00025,
  "optimizer": "tensorflow.python.training.rmsprop.RMSPropOptimizer",
  "double_dqn": false,
  "optimizer_kwargs": {
    "momentum": 0.95,
    "epsilon": 0.01
  },
  "exploration": {
    "type": "epsilon_decay",
    "kwargs": {
      "epsilon": 1.0,
      "epsilon_final": 0.1,
      "epsilon_timesteps": 1e6
    }
  },
  "preprocessing": [
    {
      "type": "image_resize",
      "kwargs": {
        "width": 84,
        "height": 84
      }
    },
    {
      "type": "grayscale"
    },
    {
      "type": "center"
    },
    {
      "type": "sequence",
      "kwargs": {
        "length": 4
      }
    }
  ],
  "repeat_actions": 1
}
